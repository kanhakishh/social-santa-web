{
    "name": "exrs-website",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start:dev": "env-cmd -f dev.env next dev  --turbopack",
        "start:prod": "env-cmd -f prod.env next dev --turbopack",
        "build:dev": "env-cmd -f dev.env next build",
        "build:prod": "env-cmd -f prod.env next build",
        "build": "next build",
        "serve": "next start",
        "lint": "eslint .",
        "format": "prettier --write .",
        "prepare": "husky",
        "deploy:dev": "env-cmd -f dev.env next build && aws --profile exrs s3 sync out/ s3://dev-exclusive-resell.com --delete && aws --profile exrs cloudfront create-invalidation --distribution-id E3N6JUKNSVRJT4 --paths \"/*\""
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write",
            "git add ."
        ]
    },
    "dependencies": {
        "@hookform/resolvers": "^3.10.0",
        "@radix-ui/react-dialog": "^1.1.4",
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-navigation-menu": "^1.2.3",
        "@radix-ui/react-scroll-area": "^1.2.2",
        "@radix-ui/react-separator": "^1.1.1",
        "@radix-ui/react-slot": "^1.1.1",
        "@radix-ui/react-tabs": "^1.1.2",
        "@radix-ui/react-toast": "^1.2.4",
        "@types/react-star-ratings": "^2.3.3",
        "axios": "^1.7.9",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "embla-carousel-react": "^8.5.1",
        "lucide-react": "^0.469.0",
        "next": "15.1.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.54.2",
        "react-star-ratings": "^2.3.0",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "vaul": "^1.1.2",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@eslint/js": "^9.17.0",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@typescript-eslint/eslint-plugin": "^8.18.2",
        "@typescript-eslint/parser": "^8.18.2",
        "autoprefixer": "^10.4.20",
        "env-cmd": "^10.1.0",
        "eslint": "^9",
        "eslint-config-next": "15.1.2",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.3",
        "eslint-plugin-react-hooks": "^5.1.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.11",
        "postcss": "^8",
        "prettier": "^3.4.2",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    }
}
